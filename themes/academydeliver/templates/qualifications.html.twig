{% embed 'partials/base.html.twig' %}

  {% block content %}

    {% if page.header.banner_image %}
      {% include 'partials/header_image.html.twig' %}
    {% endif %}

    {% if config.plugins.breadcrumbs.enabled %}
      {% include 'partials/breadcrumbs.html.twig' %}
    {% endif %}

    <div class="content-wrapper g-grid pure-g-r">
        <div class="g-block pure-u-2-3" id="qualifcations-content">
          {% set page_image = page.media.images|first %}
          {% if page_image %}
            <p><img src="{{ page_image.url }}"/></p>
          {% endif %}
          
          {{ page.content }}

          {% include 'partials/qualification_regions.html.twig' %}

        </div>
        <div class="g-block size-1-3 pure-u-1-3" id="sidebar">
            {% include 'partials/sidebar.html.twig' %}
        </div>
    </div>

  {% endblock %}

{% endembed %}
